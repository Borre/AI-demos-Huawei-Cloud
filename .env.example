# Huawei Cloud AI Demos - Credentials Configuration
# Copy this file to .env and fill in your credentials for each demo

# === DeepSeek SQL Demo Credentials ===
# Database connection details
DB_HOST=localhost
DB_USER=your_db_user
DB_PASSWORD=your_db_password
DB_NAME=your_db_name

# DeepSeek API key
DEEPSEEK_API_KEY=your_deepseek_api_key

# === Huawei Cloud Image Recognition Demo Credentials ===
# Huawei Cloud Access Key and Secret Key for Image Recognition service
HUAWEI_CLOUD_ACCESS_KEY=your_access_key_here
HUAWEI_CLOUD_SECRET_KEY=your_secret_key_here
# Optional region (defaults to cn-north-4)
HUAWEI_CLOUD_REGION=cn-north-4

# === Huawei Cloud OCR Demo Credentials ===
# Huawei Cloud Access Key and Secret Key for OCR service
HUAWEICLOUD_SDK_AK=your_access_key_here
HUAWEICLOUD_SDK_SK=your_secret_key_here
# Optional region (defaults to AP_SOUTHEAST_1)
HUAWEICLOUD_SDK_REGION=AP_SOUTHEAST_1